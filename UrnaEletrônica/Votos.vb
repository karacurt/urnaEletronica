Public Class Votos

    Dim dilmavts, serravts, zemariavts, levyvts, marinavts, nulosvts As Double

    Function DilmaVotos() As Double

        dilmavts = dilma / cont

        DilmaVotos = dilmavts


    End Function
    Function SerraVotos() As Double


        serravts = serra / cont


        SerraVotos = serravts


    End Function
    Function ZemariaVotos() As Double

        zemariavts = zemaria / cont


        ZemariaVotos = zemariavts


    End Function

    Function LevyVotos() As Double


        levyvts = levy / cont


        LevyVotos = levyvts


    End Function

    Function MarinaVotos() As Double

        marinavts = marina / cont

        MarinaVotos = marinavts

    End Function
    Function NulosVotos() As Double

        nulosvts = nulos / cont


        NulosVotos = nulosvts

    End Function


End Class
